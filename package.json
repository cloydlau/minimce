{
  "name": "minimce",
  "version": "0.2.15",
  "description": "Offline, Word documents insertion supported richtext editor powered by TinyMCE",
  "keywords": [
    "tinymce",
    "offline",
    "vue",
    "richtext",
    "rich text",
    "editor",
    "word",
    "wps",
    "doc",
    "docx",
    "office",
    "WYSIWYG"
  ],
  "author": "cloydlau",
  "license": "LGPL-2.1-only",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/minimce"
  },
  "main": "./dist/minimce.umd.min.js",
  "scripts": {
    "dev": "vue-demi-switch 3 vue3 && cd ./vue3demo && pnpm dev",
    "dev:2": "vue-demi-switch 2 vue2 && cd ./vue2demo && pnpm dev",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rd /s /q .\\node_modules",
    "ri": "npm run clean && pnpm i",
    "u": "node scripts/upgrade.js",
    "r": "node scripts/release.js",
    "rs": "node scripts/release.js --straightforward"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "latest",
    "@tinymce/tinymce-vue2": "npm:@tinymce/tinymce-vue@^3.2.8",
    "cozyalert": "latest",
    "kayran": "latest",
    "lodash-es": "latest",
    "mammoth": "latest",
    "tinymce": "latest",
    "uuid": "latest",
    "vue-demi": "latest",
    "vue-global-config": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "chalk": "^4.1.2",
    "enquirer": "latest",
    "execa": "^4.1.0",
    "minimist": "latest",
    "semver": "latest",
    "typescript": "latest",
    "unocss": "latest",
    "vite": "latest",
    "vite-plugin-glob": "latest",
    "vitest": "latest"
  }
}
