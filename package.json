{
  "name": "minimce",
  "version": "0.4.10",
  "private": false,
  "description": "Rich text editor, offline TinyMCE Vue 2.6 / 2.7 / 3 wrapper, zero network delay.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/minimce",
  "keywords": [
    "tinymce",
    "offline",
    "without-network",
    "vue",
    "richtext",
    "rich text",
    "rich editor",
    "rich text editor",
    "rte",
    "editor",
    "word",
    "wps",
    "doc",
    "docx",
    "office",
    "WYSIWYG",
    "vue-demi",
    "富文本",
    "离线"
  ],
  "exports": {
    ".": {
      "require": "./dist/minimce.umd.js",
      "import": "./dist/minimce.mjs",
      "browser": "./dist/minimce.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/minimce.umd.js",
  "module": "./dist/minimce.mjs",
  "unpkg": "./dist/minimce.umd.js",
  "jsdelivr": "./dist/minimce.umd.js",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm switch3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "pnpm switch2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2": "pnpm switch2 && vite --config ./demo/vue2/vite.config.ts --open",
    "switch3": "cl switchVue 3 --vue2deps \"element-ui\" --vue3deps \"element-plus\"",
    "switch2.7": "cl switchVue 2.7 --vue2deps \"element-ui\" --vue3deps \"element-plus\"",
    "switch2": "cl switchVue 2 --vue2deps \"element-ui\" --vue3deps \"element-plus\"",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "release": "pnpm switch3 && pnpm build && cl release"
  },
  "peerDependencies": {
    "@vue/composition-api": "1",
    "tinymce": "6",
    "vue": "2 || 3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "latest",
    "uuid": "latest",
    "vue-demi": "latest",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@cloydlau/scripts": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vitest/coverage-c8": "latest",
    "@vitest/ui": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/test-utils": "latest",
    "axios": "latest",
    "axios-shortcut": "latest",
    "case-police": "latest",
    "cozyalert": "latest",
    "element-plus": "latest",
    "eslint": "latest",
    "happy-dom": "latest",
    "husky": "latest",
    "jsdom": "latest",
    "json-editor-vue": "latest",
    "lint-staged": "latest",
    "mammoth": "latest",
    "minimce": "latest",
    "qs": "latest",
    "sass": "latest",
    "tinymce": "latest",
    "typescript": "latest",
    "vanilla-jsoneditor": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitest": "latest",
    "vue": "latest"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  },
  "stackblitz": {
    "startCommand": "npm run test"
  }
}
